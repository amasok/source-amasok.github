<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "amasok's page" %></title>
    <meta name="keywords" content="<%= data.page.keywords %>">
    <meta name="description" content="<%= data.page.description %>">
    <%= stylesheet_link_tag "normalize","http://fonts.googleapis.com/css?family=Finger+Paint", "all" %>
    <%= javascript_include_tag "http://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js" %>
<script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

  ga('create', 'UA-45973666-1', 'amasok.github.io');
  ga('send', 'pageview');

</script>  
</head>
  
  <body class="<%= page_classes %> note font-finger">

<div class="wrapper">
  <%= yield %>
  <di:qv>
    &copy; Copyright 2013, amasok.
  </div>
</div>  
    <%= javascript_include_tag "all" %>
    </body>
</html>
