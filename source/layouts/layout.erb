<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "amasok's page" %></title>
    <meta name="keywords" content="<%= data.page.keywords %>">
    <meta name="description" content="<%= data.page.description %>">
    <%= stylesheet_link_tag "normalize","http://fonts.googleapis.com/css?family=Finger+Paint", "all" %>
    <%= javascript_include_tag "http://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js", "googleAnalytics" %>    
</head>
  
  <body class="<%= page_classes %> note font-finger">

<div class="wrapper">
  <%= yield %>
  <di:qv>
    &copy; Copyright 2013, amasok.
  </div>
</div>  
    <%= javascript_include_tag "all" %>
    </body>
</html>
